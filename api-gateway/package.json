{
  "name": "luontotieto-api-gateway",
  "version": "0.0.1",
  "description": "API gateway for luontotieto",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build ./dist",
    "build": "yarn clean && yarn install && tsc --build .",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "dev": "tsc --build . && concurrently --prefix '[{name}]' --names 'tsc,nodemon' 'tsc --build --preserveWatchOutput -w .' 'NODE_ENV=local nodemon dist/index.js'"
  },
  "dependencies": {
    "@node-saml/node-saml": "5.1.0",
    "@node-saml/passport-saml": "5.1.0",
    "axios": "1.12.0",
    "connect-redis": "9.0.0",
    "cookie-parser": "1.4.7",
    "csurf": "1.11.0",
    "date-fns": "4.1.0",
    "express": "5.1.0",
    "express-http-proxy": "2.1.1",
    "express-session": "1.18.2",
    "fast-xml-parser": "5.2.5",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "make-error-cause": "2.3.0",
    "nocache": "4.0.0",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "pino": "9.9.4",
    "pino-http": "10.5.0",
    "pino-pretty": "13.1.1",
    "query-string": "9.2.2",
    "redis": "5.8.2",
    "source-map-support": "0.5.21",
    "zod": "4.1.9"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@types/cookie-parser": "1.4.9",
    "@types/csurf": "1.11.5",
    "@types/express-http-proxy": "1.6.7",
    "@types/express-session": "1.18.2",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lodash": "4.17.20",
    "@types/node": "24.2.0",
    "@types/passport": "1.0.17",
    "@types/passport-local": "1.0.38",
    "@types/passport-strategy": "0.2.38",
    "@types/pino-http": "5.8.4",
    "@types/pino-pretty": "5.0.0",
    "@types/pino-std-serializers": "4.0.0",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "concurrently": "9.2.1",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.35.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "packageManager": "yarn@4.5.0"
}

{
  "name": "luontotieto-frontend",
  "version": "0.0.1",
  "description": "Frontend for luontotieto",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.1",
    "@fortawesome/free-regular-svg-icons": "6.7.1",
    "@fortawesome/free-solid-svg-icons": "6.7.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@react-spring/web": "10.0.1",
    "@tanstack/react-query": "5.84.2",
    "@types/react": "18.3.12",
    "babel-plugin-styled-components": "2.1.4",
    "classnames": "2.5.1",
    "core-js": "3.45.0",
    "date-fns": "4.1.0",
    "file-saver": "2.0.5",
    "lodash": "4.17.21",
    "polished": "4.3.1",
    "react": "18.3.1",
    "react-csv": "2.2.2",
    "react-dom": "18.3.1",
    "react-focus-lock": "2.13.6",
    "react-router-dom": "7.6.0",
    "react-tag-autocomplete": "7.5.1",
    "react-textarea-autosize": "8.5.5",
    "styled-components": "6.1.19",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/preset-env": "7.27.1",
    "@eslint/compat": "1.3.2",
    "@types/file-saver": "2.0.7",
    "@types/lodash": "4.17.20",
    "@types/node": "24.0.3",
    "@types/react-csv": "1.1.10",
    "@types/react-dom": "18.3.1",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "axios": "1.9.0",
    "babel-loader": "10.0.0",
    "concurrently": "9.1.0",
    "css-loader": "7.1.2",
    "esbuild": "0.25.9",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-expressions": "1.3.2",
    "eslint-plugin-lodash": "8.0.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "express": "5.1.0",
    "express-http-proxy": "2.1.1",
    "postcss": "8.5.6",
    "postcss-loader": "8.1.1",
    "postcss-preset-env": "10.3.0",
    "prettier": "3.6.2",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.5.0"
}

{
  "name": "luontotieto-frontend",
  "version": "0.0.1",
  "description": "Frontend for luontotieto",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.1",
    "@fortawesome/free-regular-svg-icons": "6.7.1",
    "@fortawesome/free-solid-svg-icons": "6.7.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@react-spring/web": "^9.7.4",
    "@tanstack/react-query": "^5.51.11",
    "@types/react": "^18.3.3",
    "babel-plugin-styled-components": "^2.1.4",
    "classnames": "^2.5.1",
    "core-js": "^3.38.1",
    "date-fns": "^4.1.0",
    "file-saver": "2.0.5",
    "lodash": "^4.17.21",
    "polished": "^4.3.1",
    "react": "^18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-focus-lock": "^2.13.2",
    "react-router-dom": "^7.0.2",
    "react-tag-autocomplete": "7.4.0",
    "react-textarea-autosize": "^8.5.3",
    "styled-components": "6.1.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.0",
    "@eslint/compat": "^1.2.3",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.14.0",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.29",
    "@types/uuid": "^10",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "axios": "^1.7.4",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-expressions": "^1.3.1",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "express": "^4.21.0",
    "express-http-proxy": "^2.0.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.0",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.5.0"
}
